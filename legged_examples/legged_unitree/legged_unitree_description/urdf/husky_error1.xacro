<?xml version="1.0" encoding="utf-8"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="husky">

  <xacro:arg name="robot_type" default="husky"/>

  <xacro:property name="motor_damping" value="0.2428" />
  <xacro:property name="free_joint_damping" value="0.005" />

  <xacro:property name="motor_sfriction" value="0.1" />
  <xacro:property name="free_joint_sfriction" value="0.005" />

  <xacro:property name="r_hip_frontal_ipos" value="0.0440" />
  <xacro:property name="l_hip_frontal_ipos" value="3.0976" />
  <xacro:property name="l_hip_frontal_lowlim" value="-0.1745" />
  <xacro:property name="l_hip_frontal_uplim" value="1.3963" />
