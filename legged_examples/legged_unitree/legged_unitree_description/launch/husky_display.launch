<launch>
	<!--Spawn robot, rviz, and publishing nodes, standard procedure-->
    	<param name="robot_description" command="xacro $(find legged_unitree_description)/urdf/husky.xacro"/>
    
    	<node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" />
	<node name="rviz" pkg="rviz" type="rviz" args="$(find legged_unitree_description)/launch/config.rviz" />
	<node pkg="joint_state_publisher_gui" type="joint_state_publisher_gui" name="joint_state_publisher_gui" output="screen"/>  
	
</launch>
