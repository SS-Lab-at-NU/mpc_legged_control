    <dynamics damping="${motor_damping}" friction="${motor_sfriction}"/>
    <limit effort="${efflim}" lower="${l_hip_sagittal_lowlim}" upper="${l_hip_sagittal_uplim}" velocity="8.7267"/>
  </joint>

  <xacro:property name="l_knee_ipos" value="1.6755" />
  <xacro:property name="r_knee_ipos" value="-1.6755" />    
  <xacro:property name="l_knee_lowlim" value="-0.6981" />
  <xacro:property name="l_knee_uplim" value="0.6981" />

