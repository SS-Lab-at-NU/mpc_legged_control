  <xacro:property name="r_hip_sagittal_ipos" value="-2.0944" />
  <xacro:property name="l_hip_sagittal_lowlim" value="-0.8727" />
  <xacro:property name="l_hip_sagittal_uplim" value="0.8727" />

  <joint name="RF_HFE" type="revolute">
