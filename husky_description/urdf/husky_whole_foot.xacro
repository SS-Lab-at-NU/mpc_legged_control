    <axis xyz="0 0 1" />
    <dynamics damping="${motor_damping}" friction="${motor_sfriction}"/>
    <limit effort="${efflim}" lower="${l_hip_frontal_lowlim}" upper="${l_hip_frontal_uplim}" velocity="8.7267"/>
  </joint>

