    <axis xyz="0.0 0.0 ${l_axis}" />
    <dynamics damping="${motor_damping}" friction="${motor_sfriction}"/>
    <limit effort="${efflim}" lower="${l_hip_sagittal_lowlim}" upper="${l_hip_sagittal_uplim}" velocity="8.7267"/>
